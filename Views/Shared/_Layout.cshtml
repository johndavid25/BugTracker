                        {
                            <li>
                                <a asp-controller="Projects" asp-action="ManageUsersOnProjects">
                                    <span class="nav-link-icon">
                                        <i data-feather="users"></i>
                                    </span>
                                    <span>Project Management</span>
                                </a>
                            </li>

                        }
                        else if (User.IsInRole("ProjectManager"))
                        {
                            <li>
                                <a asp-controller="Projects" asp-action="ManageUsersOnProjects">
                                    <span class="nav-link-icon">
                                        <i data-feather="users"></i>
                                    </span>
                                    <span>Project Management</span>
                                </a>
                            </li>

                        }
                        <li>
                            <a asp-controller="Projects" asp-action="Index">
                                <span class="nav-link-icon">
                                    <i data-feather="folder"></i>
                                </span>
                                <span>Projects</span>
                            </a>
                        </li>
                        <li>
                            <a asp-controller="Projects" asp-action="MyProjects">
                                <span class="nav-link-icon">
                                    <i data-feather="folder-plus"></i>
                                </span>
                                <span>My Projects</span>
                            </a>
                        </li>
                        <li>
                            <a asp-controller="Tickets" asp-action="Index">
                                <span class="nav-link-icon">
                                    <i data-feather="file"></i>
                                </span>
                                <span>Tickets</span>
                            </a>
                        </li>
                        <li>
                            <a asp-controller="Tickets" asp-action="MyTickets">
                                <span class="nav-link-icon">
                                    <i data-feather="file-text"></i>
                                </span>
                                <span>My Tickets</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <!-- end::navigation -->
            <!-- Content body -->
            <div class="content-body">
                <div class="content">
                    @RenderBody()
                </div>

                <!-- Footer -->
                <footer class="content-footer">
                    <div>© 2021 John Williams/Gogi - <a href="http://laborasyon.com" target="_blank">Laborasyon</a></div>
                    <div>
                        <nav class="nav">
                            <a href="//themeforest.net/licenses/standard" target="_blank" class="nav-link">Licenses</a>
                            <a href="//github.com/johndavid25/BugTracker" target="_blank" class="nav-link">GitHub</a>
                            <a href="//johndevportfolio.netlify.app" target="_blank" class="nav-link">Portfolio</a>
                        </nav>
                    </div>
                </footer>
                <!-- ./ Footer -->
            </div>
            <!-- ./ Content body -->
        </div>
        <!-- ./ Content wrapper -->
    </div>
    <!-- ./ Layout wrapper -->
    <!-- Main scripts -->
    <script src="~/js/bundle.js"></script>

    <!-- Apex chart -->
    <script src="~/js/apexcharts.min.js"></script>

    <!-- Daterangepicker -->
    <script src="~/js/daterangepicker.js"></script>

    <!-- DataTable -->
    <script src="~/js/datatables.min.js"></script>

    <!-- Dashboard scripts -->
    <script src="~/js/dashboard.js"></script>

    <!-- Tagsinput -->
    <script src="~/js/bootstrap-tagsinput.js"></script>
    <script src="~/js/tagsinput.js"></script>

    <!-- quill -->
    <script src="~/js/quill.js"></script>

    <!-- Mail example -->
    <script src="~/js/mail.js"></script>

    <!-- App scripts -->
    <script src="~/js/app.min.js"></script>
    <script src="~/js/dropzone.js"></script>

    <!--Summernote Editor-->
    <script src="//cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>



